U0 EndAllTutorialVar()
{
  dialogue_num=0;
  help_in_progress=FALSE;
  current_help_id=-1;
  dialogue_hold_timer=0;
  tutorial_dialogue_txt="";
  trigger_event_delay=0;
  sprite_nervous=FALSE;
}

U0 TutorialNextDialogue()
{
  dialogue_num++;
  dialogue_hold_timer=0;
  tutorial_sprite_timer=9;
}

U0 ToggleHIP(I64 help_id)
{
  help_in_progress=TRUE;
  current_help_id=help_id;
  if (tutorial_sprite_timer>9) {
    tutorial_sprite_timer=10;
  }
}

U0 AnimateTaskLS0(I64)
{
  I64 i=0;
  Bool misc_help=FALSE;
  while (TRUE) {
    if (player_speed_breaker_lock) {//...
      if (player_speed_breaker_recharge_minus>DEFAULT_PLAYER_SPEED_BREAKER_RECHARGE_COOLDOWN-10) {
	player_speed_breaker_lock=FALSE;
      }//unlock after ltpurple
      player_speed_breaker_units=0;
    }
    if (trigger_event_delay<60) {
      trigger_event_delay++;
    }
    if (!help_in_progress && tutorial_sprite_timer>0) {
      tutorial_sprite_timer--;
    }
    if (trigger_event_delay>=60) {
    if (!help_in_progress) {
      if (collected_help<3) {//default conditions
	if (player_speed_breaker_lock && player_speed_breaker_recharge_minus<DEFAULT_PLAYER_SPEED_BREAKER_RECHARGE_COOLDOWN/2-10) {
          player_speed_breaker_recharge_minus=0;
	}
        for (i=0;i<HELP_PLAYER_NUM;i++) {      //help player
          if (!HelpPlayer[i].toggled && 
               GetDistance(player_x,player_y,HelpPlayer[i].x+PLAYER_WIDTH/2,HelpPlayer[i].y+PLAYER_HEIGHT/2)<=20) {
	    if (i<3) {
	      collected_help++;
            }
            HelpPlayer[i].toggled=TRUE;
	    ToggleHIP(i);
          }
        }
      } else {//other conditions
	misc_help=FALSE;
	if (collected_help<HELP_PLAYER_NUM) {
          for (i=3;i<HELP_PLAYER_NUM;i++) {      //help player
	    if (!HelpPlayer[i].toggled && 
                 GetDistance(player_x,player_y,HelpPlayer[i].x+PLAYER_WIDTH/2,HelpPlayer[i].y+PLAYER_HEIGHT/2)<=20) {
	      collected_help++;
	      HelpPlayer[i].toggled=TRUE;
	      misc_help=TRUE;
	      ToggleHIP(i);
            }
	  }
	}
	if (!misc_help) {
        if (!independent_alpha) {
	  if (!tutorial_sbunits_hundred) {//sbunits 100
	    if (!tutorial_ltpurple_performance) {//ltpurple perf
	      if (!tutorial_combo_grade_three) {//comboing
	        if (!tutorial_blue_performance) {//blue perf
		  if (!tutorial_first_kill) {//kill
		    if (enemy_kills>0) {
		      tutorial_first_kill=TRUE;
		      ToggleHIP(HELP_PLAYER_NUM+2);
		    }
		  } else if (player_speed_breaker_recharge_minus>(DEFAULT_PLAYER_SPEED_BREAKER_RECHARGE_COOLDOWN-10)/4) {//allow blue
		    tutorial_blue_performance=TRUE;
		    ToggleHIP(HELP_PLAYER_NUM+3);
		  }
	        } else if (performance_grade_arr[0]>=2||
			   performance_grade_arr[1]>=2||
			   performance_grade_arr[2]>=2) {
	      	  tutorial_combo_grade_three=TRUE;
	      	  ToggleHIP(HELP_PLAYER_NUM+4);
	        }
	      } else if (player_speed_breaker_recharge_minus>DEFAULT_PLAYER_SPEED_BREAKER_RECHARGE_COOLDOWN-10) {
	    	tutorial_ltpurple_performance=TRUE;
	        ToggleHIP(HELP_PLAYER_NUM+5);
              }
	    } else if (player_speed_breaker_units>=150) {
