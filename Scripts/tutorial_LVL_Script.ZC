U0 DrawItLS(CTask *task,CDC *dc)
{
  I64 i=0;
  if (!change_view || (on_ground_timer<9 && (print_current_above || (!print_current_above && !print_current_below))) ||
      (on_ground_timer<1 && print_current_below)
     ) {//normal view
    for (i=0;i<HELP_PLAYER_NUM;i++) {
      if (!HelpPlayer[i].toggled) {
        Sprite3(dc,HelpPlayer[i].x+cam_x,HelpPlayer[i].y+cam_y,0,$IB,"<1>",BI=1$);
	if (i<3 && Blink) {
	  dc->color=YELLOW;
	  GrCircle(dc,HelpPlayer[i].x+cam_x,HelpPlayer[i].y+cam_y,2);
	}
      }
    }
  } else {
    F64 player_help_view_é=0,
        player_help_é_x=0,
        player_help_é_y=0;
    for (i=0;i<HELP_PLAYER_NUM;i++) {
      if (!HelpPlayer[i].toggled) {
        player_help_view_é=GetRotateAngle(HelpPlayer[i].x+PLAYER_WIDTH/2,HelpPlayer[i].y+PLAYER_HEIGHT/2);
        if (print_current_above) {
          player_help_é_x=GetDistance(HelpPlayer[i].x+PLAYER_WIDTH/2,HelpPlayer[i].y+PLAYER_HEIGHT/2,0,0)*Cos(player_help_view_é-Ground[on_ground_id].angle);
          player_help_é_y=GetDistance(HelpPlayer[i].x+PLAYER_WIDTH/2,HelpPlayer[i].y+PLAYER_HEIGHT/2,0,0)*Sin(player_help_view_é-Ground[on_ground_id].angle);
        } else {
          player_help_é_x=GetDistance(HelpPlayer[i].x+PLAYER_WIDTH/2,HelpPlayer[i].y+PLAYER_HEIGHT/2,0,0)*Cos(pi+player_help_view_é-Ground[on_ground_id].angle);
          player_help_é_y=GetDistance(HelpPlayer[i].x+PLAYER_WIDTH/2,HelpPlayer[i].y+PLAYER_HEIGHT/2,0,0)*Sin(pi+player_help_view_é-Ground[on_ground_id].angle);
        }
        Sprite3(dc,GR_WIDTH/2+player_help_é_x-player_é_x,GR_HEIGHT/2+player_help_é_y-player_é_y,0,$IB,"<1>",BI=1$);
      }
    }
  }
  if (tutorial_sprite_timer>0 && player_health>0) {//draw mascot sprite
    dc->color=player_color;
    if (1<=tutorial_sprite_timer<=3) {
      Sprite3(dc,-16,-120,0,$IB,"<4>",BI=4$);
    } else if (4<=tutorial_sprite_timer<=5) {
      Sprite3(dc,-16,(GR_HEIGHT/20-120)/2,0,$IB,"<4>",BI=4$);
    } else if (6<=tutorial_sprite_timer<=8) {
      Sprite3(dc,-16,GR_HEIGHT-480+24,0,$IB,"<3>",BI=3$);
    } else if (tutorial_sprite_timer>=9) {//Dialogue
      if (!sprite_nervous) {
	Sprite3(dc,-16,GR_HEIGHT-480+24,0,$IB,"<2>",BI=2$);
      } else {
	Sprite3(dc,-16,GR_HEIGHT-480+24,0,$IB,"<5>",BI=5$);
      }
      Highlight((player_color!=WHITE && player_color!=LTGREEN && player_color!=LTCYAN && player_color!=YELLOW),BLACK,WHITE,dc);//dialogue around center
      GrPrint(dc,8,GR_HEIGHT/2-8,"%s",tutorial_dialogue_txt);

      GrPrint(dc,7,GR_HEIGHT/2-10,"%s",tutorial_dialogue_txt);
      GrPrint(dc,5,GR_HEIGHT/2-10,"%s",tutorial_dialogue_txt);

      GrPrint(dc,7,GR_HEIGHT/2-9,"%s",tutorial_dialogue_txt);
      GrPrint(dc,5,GR_HEIGHT/2-11,"%s",tutorial_dialogue_txt);

      GrPrint(dc,7,GR_HEIGHT/2-11,"%s",tutorial_dialogue_txt);
      GrPrint(dc,5,GR_HEIGHT/2-9,"%s",tutorial_dialogue_txt);

      Highlight((player_color!=WHITE && player_color!=LTGREEN && player_color!=LTCYAN && player_color!=YELLOW),WHITE,BLACK,dc);
      GrPrint(dc,6,GR_HEIGHT/2-10,"%s",tutorial_dialogue_txt);
      DrawDialogueHighlight(task,dc);
    }
  }  
}

