	        ToggleHIP(HELP_PLAYER_NUM+6);
		independent_alpha=TRUE;
	    }
	  }
        } else {//independent_alphas after prev
	  if (print_current_below && !tutorial_upside_down) {//upside down***
	    tutorial_upside_down=TRUE;
